{
  "name": "react-isomorphic-boilerplate",
  "version": "0.0.1",
  "description": "React isomorphic boilerplate",
  "main": "index.html",
  "scripts": {
    "build": "rm -rf server/build; babel server/src --out-dir server/build && babel shared --out-dir server/build/shared && NODE_ENV='production' webpack",
    "build-dev": "rm -rf server/build; babel server/src --out-dir server/build && babel shared --out-dir server/build/shared && NODE_ENV='development' webpack",
    "watch": "rm -rf server/build; babel server/src --out-dir server/build && babel shared --out-dir server/build/shared && NODE_ENV='development' webpack --watch",
    "start-dev": "node bin/www"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.guava.by/aleksei/test-task.git"
  },
  "keywords": [
    "react",
    "isomorphic",
    "boilerplate",
    "universal"
  ],
  "author": "Aleksei Schurak",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.13.4",
    "hyphenation.en-us": "^0.2.1",
    "hypher": "^0.2.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.4.1",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-autoprefix": "^0.2.6",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators": "^6.6.5",
    "babel-plugin-transform-es2015-modules-amd": "^6.6.5",
    "babel-plugin-transform-es2015-typeof-symbol": "^6.6.0",
    "babel-plugin-transform-function-bind": "^6.5.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.0",
    "kouto-swiss": "^0.12.0",
    "react-hot-loader": "^1.3.0",
    "remove-webpack-plugin": "^1.1.0",
    "style-loader": "^0.13.1",
    "stylus-loader": "^2.0.0",
    "webpack": "^1.13.0"
  }
}
